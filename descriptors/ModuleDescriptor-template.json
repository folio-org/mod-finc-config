{
  "id": "${artifactId}-${version}",
  "name": "mod-finc-config",
  "provides": [
    {
      "id": "metadata-sources",
      "version": "0.1",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/metadata-sources"
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/metadata-sources/{id}"
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/metadata-sources"
        },
        {
          "methods": [
            "PUT"
          ],
          "pathPattern": "/metadata-sources/{id}"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/metadata-sources/{id}"
        }
      ]
    },
    {
      "id": "metadata-collections",
      "version": "0.1",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/metadata-collections"
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/metadata-collections/{id}"
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/metadata-collections"
        },
        {
          "methods": [
            "PUT"
          ],
          "pathPattern": "/metadata-collections/{id}"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/metadata-collections/{id}"
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.2",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST",
            "DELETE",
            "GET"
          ],
          "pathPattern": "/_/finc/tenant"
        }
      ]
    }

  ],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerArgs": {
      "HostConfig": {
        "PortBindings": {
          "8081/tcp": [
            {
              "HostPort": "%p"
            }
          ]
        }
      }
    },
    "dockerPull": false
  }
}